{
  "name": "web-app",
  "version": "1.0.0",
  "description": "Travel planning web application",
  "type": "module",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "debug": "node --inspect server/server.js",
    "build": "parcel build src/index.html --dist-dir ./dist",
    "killport": "kill $(lsof -t -i:9876) || true",
    "killport9877": "kill $(lsof -t -i:9877) || true",
    "killport3000": "kill $(lsof -t -i:3000) || true"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "nodemon": "^3.1.9",
    "parcel": "^2.14.4",
    "postcss": "^8.4.31"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ]
}
